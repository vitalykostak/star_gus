        "react-virtuoso": "^4.6.2"
    },
    "loki": {
        "pixelmatch": {
            "threshold": 0.2
        },
        "configurations": {
            "chrome.laptop": {
                "target": "chrome.docker",
                "width": 1366,
                "height": 768,
                "deviceScaleFactor": 1,
                "mobile": false
            },
            "chrome.iphone7": {
                "target": "chrome.docker",
                "preset": "iPhone 7"
            }
        }
