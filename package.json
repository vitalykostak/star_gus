{
    "name": "y",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "npx webpack serve --env port=3001 --env mode=development apiUrl=http://localhost:8000",
        "start:dev-server": "node ./json-server/index.js",
        "start:dev": "concurrently \"npm run start:dev-server\" \"npm run start\"",
        "start:vite": "vite --port 3002",
        "start:vite:dev": "concurrently \"npm run start:dev-server\" \"npm run start:vite\"",
        "build:prod": "npx webpack  --env mode=production apiUrl=https://production-project-fake-server-smoky.vercel.app",
        "build:dev": "npx webpack  --env mode=development apiUrl=http://localhost:8000",
        "lint:ts": "./node_modules/eslint/bin/eslint.js '**/*.{ts,tsx}'",
        "lint:ts:fix": "./node_modules/eslint/bin/eslint.js --fix '**/*.{ts,tsx}'",
        "lint:scss": "./node_modules/stylelint/bin/stylelint.mjs '**/*.scss'",
        "lint:scss:fix": "./node_modules/stylelint/bin/stylelint.mjs '**/*.scss' --fix",
        "test:unit": "jest --config=./config/jest/jest.config.ts",
        "test:ui": "./node_modules/loki/bin/loki test --host $(ipconfig getifaddr en0)",
        "test:ui:husky": "./node_modules/loki/bin/loki test --host $(ipconfig getifaddr en0) --requireReference",
        "test:ui:update": "./node_modules/loki/bin/loki update --host $(ipconfig getifaddr en0)",
        "test:ui:ok": "./node_modules/loki/bin/loki approve --host $(ipconfig getifaddr en0)",
        "test:ui:ci": "./node_modules/loki/bin/loki --requireReference --reactUri file:./storybook-static",
        "test:ui:report": "npm run test:ui:generate_json && npm run test:ui:generate_html",
        "test:ui:generate_json": "rm ./.loki/report.json & node scripts/generate-visual-json-report.js",
        "test:ui:generate_html": "rm ./.loki/report.html & ./node_modules/reg-cli/dist/cli.js --from .loki/report.json --report .loki/report.html",
        "format:ts": "prettier **/*{.ts,.tsx,.json,.js,.mjs} --check",
        "format:ts:fix": "prettier **/*.{ts,tsx,json,js,mjs} --write --ignore-unknown",
        "storybook": "./node_modules/storybook/index.js dev -p 6006 -c config/storybook",
        "storybook:build": "./node_modules/storybook/index.js build -c config/storybook",
        "prepare": "husky install",
        "generate-feature-slice": "node ./scripts/generate-feature-slice/index.mjs",
        "remove-feature-flag": "node ./scripts/removeToggleFeature/removeToggleFeature.mjs",
        "cy:run": "cypress run",
        "cy:open": "cypress open",
        "postinstall": "node ./scripts/clear-babel-cache/clear-babel-cache.mjs"
    },
    "author": "",
    "license": "ISC",
